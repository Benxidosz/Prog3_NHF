<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
      prefWidth="400.0" scaleShape="false" stylesheets="@../../basicStyle.css" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="ges.menu.EditorMenu">
    <children>
        <Button fx:id="newGraph" layoutX="100.0" layoutY="100.0" mnemonicParsing="false" onAction="#makeNew"
                prefHeight="100.0" prefWidth="200.0" styleClass="Button" text="New" textAlignment="CENTER">
            <font>
                <Font name="Garuda" size="32.0"/>
            </font>
        </Button>
        <Button layoutX="100.0" layoutY="250.0" mnemonicParsing="false" onAction="#load" prefHeight="100.0"
                prefWidth="200.0" styleClass="Button" text="Modify" textAlignment="CENTER">
            <font>
                <Font name="Garuda" size="32.0"/>
            </font>
        </Button>
        <Button fx:id="backEditor" layoutX="100.0" layoutY="400.0" mnemonicParsing="false" onAction="#back"
                prefHeight="100.0" prefWidth="200.0" styleClass="Button" text="Back">
            <font>
                <Font size="32.0"/>
            </font>
        </Button>
    </children>
</Pane>
